{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--AppName", "testname",
                "--GroupId", "testgroup"
            ],
            "console": "integratedTerminal"
        }
    ]
}